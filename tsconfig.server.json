{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./api-dist",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "include": [
    "api"
  ],
  "exclude": [
    "src"
  ]
}
